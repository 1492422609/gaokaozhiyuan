<view class="img">
  <view class="img2">
    <span>
      <image src="../../images/tupian2.png"></image>
      <span class="i5">智能筛选</span>
     
      <span bindtap="">
        <text class="i6" decode="{{true}}">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
          &nbsp;&nbsp;&nbsp;&nbsp;从高到低 </text>
        <image class="i7" src="../../images/shangyi.png"></image>
