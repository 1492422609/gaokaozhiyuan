            <text decode="{{true}}">&nbsp;&nbsp;</text>
            <label>
              <radio value="专科" />专科</label>
          </radio-group>
        </view>
      </view>


      <view class="s2">
        <text class="s1">所在地区</text>
        <picker bindchange="bindregionChange" class="reg" mode="selector" range="{{regiontype}}" value="{{index}}">
          <view class="region" name="region">{{regiontype[index]}}
            <image class="arrow-r diqu" bindtap="tap2" src="../../images/arrow-r-0.png"></image>
          </view>
        </picker>

        <!-- peopleHide -->
        <view class="s6">

        </view>
